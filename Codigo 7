def mostrar_tablas():
 """
Muestra las tablas de multiplicar del 1 al 9.
 """
 for i in range(1, 10):
     print(f"Tabla del {i}")
     print("-" * 15)
     for j in range(1, 11):
         print(f"{i} x {j} = {i * j}")
     print()

# Programa principal
if __name__ == "__main__":
     mostrar_tablas()
